capture_directory: ~
fixed_frame: map
target_frame: scouting_base_link
fix_orientation: false
rotate_90: false
enable_antialiasing: true
show_displays: true
show_status_bar: true
show_capture_tools: true
window_width: 1853
window_height: 1025
view_scale: 0.01101858
offset_x: -0.643132
offset_y: -1.5309
use_latest_transforms: false
background: "#a0a0a4"
displays:
  - type: mapviz_plugins/tile_map
    name: Google maps
    config:
      visible: true
      collapsed: true
      custom_sources:
        - base_url: http://localhost:8080/wmts/gm_layer/gm_grid/{level}/{x}/{y}.png
          max_zoom: 19
          name: Google
          type: wmts
      bing_api_key: ""
      source: Google
  - type: mapviz_plugins/tf_frame
    name: base_station
    config:
      visible: false
      collapsed: true
      frame: map
      color: "#ffaa00"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 478
  - type: mapviz_plugins/navsat
    name: Best GPS
    config:
      visible: false
      collapsed: true
      topic: /piksi_receiver/navsatfix_best_fix
      color: "#aa0000"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
  - type: mapviz_plugins/navsat
    name: RTK Float
    config:
      visible: false
      collapsed: true
      topic: /piksi_receiver/navsatfix_rtk_float
      color: "#ffaa00"
      draw_style: points
      position_tolerance: 0
      buffer_size: 200
  - type: mapviz_plugins/navsat
    name: RTK Fix
    config:
      visible: false
      collapsed: true
      topic: /piksi_receiver/navsatfix_rtk_fix
      color: "#55ff00"
      draw_style: points
      position_tolerance: 0.5
      buffer_size: 0
  - type: mapviz_plugins/navsat
    name: GPS filtered NavSat node
    config:
      visible: false
      collapsed: true
      topic: /gps/filtered
      color: "#0055ff"
      draw_style: points
      position_tolerance: 0.5
      buffer_size: 0
  - type: mapviz_plugins/odometry
    name: EKF result (Map)
    config:
      visible: false
      collapsed: true
      topic: /odometry/filtered/global
      color: "#55aaff"
      draw_style: arrows
      position_tolerance: 0
      buffer_size: 60
      show_laps: false
      show_covariance: false
      static_arrow_sizes: true
      arrow_size: 51
      show_timestamps: 0
  - type: mapviz_plugins/tf_frame
    name: base_link
    config:
      visible: true
      collapsed: true
      frame: scouting_base_link
      color: "#0000ff"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/tf_frame
    name: receiver
    config:
      visible: false
      collapsed: true
      frame: gps_receiver
      color: "#00aa00"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/tf_frame
    name: attitude
    config:
      visible: false
      collapsed: true
      frame: gps_attitude
      color: "#b90000"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/image
    name: rgb_bb
    config:
      visible: true
      collapsed: true
      topic: /mapping_0/debug/rgb_bb
      anchor: top left
      units: pixels
      offset_x: 0
      offset_y: 0
      width: 560
      height: 315
      keep_ratio: true
      image_transport: default
  - type: mapviz_plugins/image
    name: projection
    config:
      visible: true
      collapsed: true
      topic: /mapping_0/debug/projected
      anchor: top left
      units: pixels
      offset_x: 560
      offset_y: 0
      width: 560
      height: 315
      keep_ratio: true
      image_transport: default
  - type: mapviz_plugins/odometry
    name: GPS odometry (Map)
    config:
      visible: false
      collapsed: true
      topic: /odometry/filtered/global
      color: "#ff00ff"
      draw_style: points
      position_tolerance: 0.5
      buffer_size: 0
      show_laps: false
      show_covariance: false
      static_arrow_sizes: true
      arrow_size: 25
      show_timestamps: 0
  - type: mapviz_plugins/tf_frame
    name: camera_0
    config:
      visible: true
      collapsed: true
      frame: camera_0_base_link
      color: "#5c3566"
      draw_style: points
      position_tolerance: 0.5
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/tf_frame
    name: camera_1
    config:
