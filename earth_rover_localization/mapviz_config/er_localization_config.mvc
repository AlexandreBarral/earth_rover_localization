      size: 9
      buffer_size: 1
      alpha: 1
      color_transformer: Flat Color
      min_color: "#edd400"
      max_color: "#000000"
      value_min: 0
      value_max: 100
      use_rainbow: false
      use_automaxmin: false
      unpack_rgb: false
  - type: mapviz_plugins/pointcloud2
    name: origins_2
    config:
      visible: false
      collapsed: true
      topic: /mapping_2/pcl/odom_origins
      size: 9
      buffer_size: 1
      alpha: 1
      color_transformer: Flat Color
      min_color: "#f57900"
      max_color: "#000000"
      value_min: 0
      value_max: 100
      use_rainbow: false
      use_automaxmin: false
      unpack_rgb: false
