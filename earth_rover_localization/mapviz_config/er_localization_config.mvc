      color: "#5c3566"
      draw_style: points
      position_tolerance: 0.5
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/pointcloud2
    name: origins_0
    config:
      visible: false
      collapsed: true
      topic: /mapping_0/pcl/odom_origins
      size: 9
      buffer_size: 1
      alpha: 1
      color_transformer: Flat Color
      min_color: "#f57900"
      max_color: "#000000"
      value_min: 0
      value_max: 100
      use_rainbow: false
      use_automaxmin: false
      unpack_rgb: false
  - type: mapviz_plugins/pointcloud2
    name: origins_1
    config:
